.header {
  margin: 28px 0 0;
  display: flex;
  justify-content: space-between;
  align-content: baseline;
  flex-basis: auto;
  width: 92.5%;
}

@media screen and (max-width: 811px) {
  .header {
    width: calc(100% - 48px);
    margin: 28px 24px 0;
  }
}

@media screen and (max-width: 450px) {
  .header {
    width: calc(100% - 32px);
    margin: 16px 16px 0;
  }
}
