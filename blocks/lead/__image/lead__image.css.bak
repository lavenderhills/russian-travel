.lead__image {
  width: 77%;
  margin: 64px auto 0;
}

@media screen and (max-width: 1024px) {
  .lead__image {
    width: 91%;
    margin: 56px auto 0;
  }
}

@media screen and (max-width: 768px) {
  .lead__image {
    width: 100%;
    margin: 56px auto 0;
  }
}

@media screen and (max-width: 320px) {
  .lead__image {
    width: 100%;
    margin: 40px 0 0;
  }
}